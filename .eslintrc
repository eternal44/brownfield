{
  "parser": "babel-eslint",
  "rules": {
    "no-unused-vars": [1, {
      "ignore": ["React"]
    }],
    "indent": [
        2,
        2
      ],
    "quotes": [
      2,
      "single"
    ],
    "strict": 0,
    "comma-dangle": 0
    },
  "env": {
    "browser": true,
    "node": true,
    "mocha": true,
    "jasmine": true,
    "phantomjs": true,
    "es6": true
  },
  "extends": "eslint:recommended",
  "ecmaFeatures": {
    "modules": true
  },
  "globals": {
  }
}
